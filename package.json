{
  "name": "usgs-lst-loader",
  "productName": "usgs-lst-loader",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "set APP_DEV=true && electron-forge start",
    "release": "electron-builder",
    "package": "electron-forge package && npm run copy-env-file",
    "build-python": "pyinstaller .\\src\\backend\\calculation\\calculation.py --onefile --distpath .\\public\\tasks --add-data .\\pyenv\\Lib\\site-packages\\grapheme\\data\\grapheme_break_property.json;grapheme\\data",
    "build": "electron-builder build --pd out/usgs-lst-loader-win32-x64",
    "copy-env-file": "copy \"./.env\" \"out/usgs-lst-loader-win32-x64\"",
    "make-squirell": "electron-forge make --targets @electron-forge/maker-squirrel",
    "make-wix": "electron-forge make --targets @electron-forge/maker-wix",
    "make-nsis": "npm run package && npm run build",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "build": {
    "appId": "test",
    "win": {
      "target": "nsis",
      "publish": [
        "github"
      ]
    },
    "nsis": {
      "include": "installer.nsh",
      "allowToChangeInstallationDirectory": false,
      "oneClick": true,
      "packElevateHelper": false,
      "uninstallerIcon": "icon.ico",
      "installerIcon": "icon.ico"
    }
  },
  "keywords": [],
  "author": "shkid",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "extraResource": "./public"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "usgs_lst_loader"
          }
        },
        {
          "name": "@electron-forge/maker-wix",
          "config": {
            "language": 1033,
            "manufacturer": "My Awesome Company'"
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "devContentSecurityPolicy": "style-src-elem https://api.mapbox.com/ 'unsafe-inline'",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/entry-points/index.html",
                  "js": "./src/entry-points/renderer.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "./preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.63",
    "@electron-forge/maker-deb": "6.0.0-beta.63",
    "@electron-forge/maker-rpm": "6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "6.0.0-beta.63",
    "@electron-forge/maker-wix": "6.0.0-beta.63",
    "@electron-forge/maker-zip": "6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "6.0.0-beta.63",
    "@types/lodash.debounce": "4.0.7",
    "@types/mapbox-gl": "2.7.2",
    "@types/react": "17.0.41",
    "@types/react-calendar": "3.5.1",
    "@types/react-datepicker": "4.3.4",
    "@types/react-dates": "21.8.0",
    "@types/react-dom": "17.0.14",
    "@types/react-redux": "^7.1.24",
    "@types/redux-logger": "^3.0.9",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "5.15.0",
    "@typescript-eslint/parser": "5.15.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "css-loader": "6.7.1",
    "electron": "17.1.2",
    "electron-builder": "23.3.3",
    "electron-packager": "15.5.1",
    "eslint": "8.11.0",
    "eslint-plugin-import": "2.25.4",
    "fork-ts-checker-webpack-plugin": "6.5.0",
    "node-loader": "2.0.0",
    "style-loader": "3.3.1",
    "ts-loader": "9.2.8",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0",
    "@chakra-ui/react": "^1.8.9",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@fortawesome/react-fontawesome": "0.1.18",
    "@reduxjs/toolkit": "1.9.7",
    "@turf/turf": "6.5.0",
    "7zip-bin": "5.2.0",
    "axios": "0.27.2",
    "chokidar": "^3.5.3",
    "dotenv": "^16.0.2",
    "electron-dl": "^3.5.0",
    "electron-download-manager": "^2.1.2",
    "electron-squirrel-startup": "1.0.0",
    "electron-store": "^8.1.0",
    "electron-typescript-ipc": "^1.0.1",
    "electron-updater": "^5.3.0",
    "framer-motion": "^6.5.1",
    "fs": "0.0.1-security",
    "lodash": "4.17.21",
    "lodash.debounce": "4.0.8",
    "mapbox-gl": "^2.8.1",
    "moment": "2.18.1",
    "path-browserify": "1.0.1",
    "python-shell": "3.0.1",
    "react": "17.0.2",
    "react-calendar": "3.7.0",
    "react-dom": "17.0.2",
    "react-map-gl": "^7.0.19",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux-logger": "^3.0.6",
    "socket.io-client": "4.4.1",
    "styled-components": "5.3.5",
    "throttle-debounce": "^5.0.0",
    "util": "^0.12.5"
  }
}
