{
  "name": "usgs-lst-loader",
  "productName": "usgs-lst-loader",
  "version": "2.2.4",
  "description": "My Electron application description",
  "main": ".vite/main/index.js",
  "scripts": {
    "dev:renderer": "cross-env APP_DEV=true rspack serve --config rspack.renderer.config.js",
    "dev:main": "cross-env APP_DEV=true rspack build --config rspack.main.config.js --watch",
    "dev:preload": "cross-env APP_DEV=true rspack build --config rspack.preload.config.js --watch",
    "start": "concurrently \"npm run dev:renderer\" \"npm run dev:main\" \"npm run dev:preload\" \"wait-on http://localhost:5173 .vite/main/index.js && cross-env APP_DEV=true electron .\"",
    "build-python": "pyinstaller .\\backend\\calculation\\calculation.py --onefile --distpath .\\public\\tasks --add-data .\\pyenv\\Lib\\site-packages\\grapheme\\data\\grapheme_break_property.json;grapheme\\data",
    "build:main": "cross-env APP_DEV=false rspack build --config rspack.main.config.js",
    "build:preload": "cross-env APP_DEV=false rspack build --config rspack.preload.config.js",
    "build:renderer": "cross-env APP_DEV=false rspack build --config rspack.renderer.config.js",
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "make-nsis": "npm run build && electron-builder",
    "release-nsis": "npm run build && electron-builder --publish always",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "repository": "https://github.com/nzaycev/STAGE",
  "build": {
    "appId": "nzaycev.stage",
    "productName": "Stage",
    "asar": true,
    "files": [
      ".vite/**/*",
      "package.json",
      "!**/*.map",
      "!**/node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "./public",
        "to": "public"
      }
    ],
    "win": {
      "target": "nsis",
      "publish": {
        "provider": "github",
        "owner": "nzaycev",
        "repo": "ASAP"
      }
    },
    "nsis": {
      "include": "installer.nsh",
      "allowToChangeInstallationDirectory": false,
      "oneClick": true,
      "packElevateHelper": false,
      "uninstallerIcon": "icon.ico",
      "installerIcon": "icon.ico"
    }
  },
  "keywords": [],
  "author": "shkid",
  "license": "MIT",
  "devDependencies": {
    "@rspack/cli": "^1.6.5",
    "@rspack/core": "^1.6.5",
    "@types/lodash.debounce": "4.0.7",
    "@types/mapbox-gl": "2.7.2",
    "@types/node": "^20.10.6",
    "@types/react": "17.0.41",
    "@types/react-calendar": "3.5.1",
    "@types/react-datepicker": "4.3.4",
    "@types/react-dates": "21.8.0",
    "@types/react-dom": "17.0.14",
    "@types/react-highlight-words": "^0.20.0",
    "@types/react-redux": "^7.1.24",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "5.15.0",
    "@typescript-eslint/parser": "5.15.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^39.2.4",
    "electron-builder": "23.3.3",
    "eslint": "8.11.0",
    "eslint-plugin-import": "2.25.4",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "tailwindcss": "^3.4.18",
    "typescript": "~4.5.4",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0",
    "@chakra-ui/react": "^1.8.9",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@fortawesome/react-fontawesome": "0.1.18",
    "@reduxjs/toolkit": "1.9.7",
    "@tailwindcss/postcss": "^4.1.17",
    "@turf/turf": "6.5.0",
    "7zip-bin": "5.2.0",
    "axios": "0.27.2",
    "chokidar": "^3.5.3",
    "dotenv": "^16.0.2",
    "electron-dl": "^3.5.0",
    "electron-download-manager": "^2.1.2",
    "electron-squirrel-startup": "1.0.0",
    "electron-store": "^8.1.0",
    "electron-typescript-ipc": "^1.0.1",
    "electron-updater": "^5.3.0",
    "fast-glob": "^3.3.3",
    "framer-motion": "^6.5.1",
    "fs": "0.0.1-security",
    "lodash": "4.17.21",
    "lodash.debounce": "4.0.8",
    "lucide-react": "^0.554.0",
    "mapbox-gl": "^2.8.1",
    "moment": "2.18.1",
    "path-browserify": "1.0.1",
    "python-shell": "3.0.1",
    "react": "17.0.2",
    "react-calendar": "3.7.0",
    "react-dom": "17.0.2",
    "react-highlight-words": "^0.21.0",
    "react-map-gl": "^7.0.19",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux-logger": "^3.0.6",
    "socket.io-client": "4.4.1",
    "throttle-debounce": "^5.0.0",
    "util": "^0.12.5"
  }
}
